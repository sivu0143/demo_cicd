---
app-front:
  serviceOfferingName: app-front
  servicePlanName: developer
html5-apps-deployer:
  env:
    SAP_CLOUD_SERVICE: {{strippedAppName}}.service
  image:
    repository: {{appName}}-html5-deployer
  bindings:
    app-front:
      serviceInstanceName: app-front
{{=<% %>=}}
  envFrom:
    - configMapRef:
        name: "{{ .Release.Name }}-app-front-configmap"
<%={{ }}=%>
backendDestinations:
  srv-api:
    service: srv
