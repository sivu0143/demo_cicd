entity my.bookshop.Books {
  key ID    : Integer;
      title : String  @title: 'Title';
      stock : Integer @title: 'Stock';
}

service CatalogService {
  entity Books as projection on my.bookshop.Books;
}

annotate CatalogService.Books with @UI.LineItem: [
  {Value: ID},
  {Value: title},
  {Value: stock},
];
