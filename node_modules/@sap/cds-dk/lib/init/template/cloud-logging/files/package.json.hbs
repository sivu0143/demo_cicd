{
  {{#withTelemetry}}
  "dependencies": {
    "@cap-js/telemetry": ">=0",
    "@grpc/grpc-js": "^1",
    "@opentelemetry/exporter-trace-otlp-grpc": "<0.200",
    "@opentelemetry/exporter-metrics-otlp-grpc": "<0.200",
    "@opentelemetry/host-metrics": "<0.200"
  },
  {{/withTelemetry}}
  "cds": {
    "requires": {
      {{^withTelemetry}}
      "cloud-logging": true
      {{/withTelemetry}}
      {{#withTelemetry}}
      "[{{profile}}]": {
        "telemetry": {
          "kind": "to-cloud-logging"
        }
      }
      {{/withTelemetry}}
    }
  }
}
